.i-panel {
    position: relative;
    overflow: hidden
}

.i-panel-title {
    font-size: 32rpx;
    line-height: 1;
    color: #333;
    padding: 20px 16px 10px;
    text-align: center;
}
.i-panel-title.white {
    color:#fff;
}
.i-panel-title-hide-top {
    padding-top: 0
}

.i-panel-content {
    position: relative;
    overflow: hidden
}

.i-panel-content .i-panel-title {
    text-align: left;
    color:#fff;
    padding:0;
}

.td-select .i-panel-title {
    font-size:40rpx;
}