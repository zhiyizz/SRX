  <view class="tl box container" id="message">
    <view class="tl box container">
      <view class="title tc title-mar" id="myapp">iBuick APP个人信息说明</view>
      <view class="time tr">更新、发布并生效于：2022年06月08日</view>
      <!--测试环境  https://iq.apps-qa.sgmlink.com/ibuick/-->      
